t\t\t\t\t<el-form-item class=\"list-item\" v-if=\"tableName=='renshizhuguan'\">\r\n\t\t\t\t\t\t<div class=\"lable\" :class=\"changeRules('zhanghao')?'required':''\">账号：</div>\r\n\t\t\t\t\t\t<el-input  v-model=\"ruleForm.zhanghao\"  autocomplete=\"off\" placeholder=\"账号\"  type=\"text\"  />\r\n\t\t\t\t\t</el-form-item>\n\t\t\t\t\t<el-form-item class=\"list-item\" v-if=\"tableName=='renshizhuguan'\">\r\n\t\t\t\t\t\t<div class=\"lable\" :class=\"changeRules('mima')?'required':''\">密码：</div>\r\n\t\t\t\t\t\t<el-input  v-model=\"ruleForm.mima\"  autocomplete=\"off\" placeholder=\"密码\"  type=\"password\"  />\r\n\t\t\t\t\t</el-form-item>\n\t\t\t\t\t<el-form-item class=\"list-item\" v-if=\"tableName=='renshizhuguan'\">\r\n\t\t\t\t\t\t<div class=\"lable\" :class=\"changeRules('mima')?'required':''\">确认密码：</div>\r\n\t\t\t\t\t\t<el-input  v-model=\"ruleForm.mima2\" autocomplete=\"off\" placeholder=\"确认密码\" type=\"password\" />\n\t\t\t\t\t</el-form-item>\n\t\t\t\t\t<el-form-item class=\"list-item\" v-if=\"tableName=='renshizhuguan'\">\r\n\t\t\t\t\t\t<div class=\"lable\" :class=\"changeRules('xingming')?'required':''\">姓名：</div>\r\n\t\t\t\t\t\t<el-input  v-model=\"ruleForm.xingming\"  autocomplete=\"off\" placeholder=\"姓名\"  type=\"text\"  />\r\n\t\t\t\t\t</el-form-item>\n\t\t\t\t\t<el-form-item class=\"list-item\" v-if=\"tableName=='renshizhuguan'\">\r\n\t\t\t\t\t\t<div class=\"lable\" :class=\"changeRules('xingbie')?'required':''\">性别：</div>\r\n\t\t\t\t\t\t<el-select v-model=\"ruleForm.xingbie\" placeholder=\"请选择性别\" >\n\t\t\t\t\t\t\t<el-option\n\t\t\t\t\t\t\t\tv-for=\"(item,index) in renshizhuguanxingbieOptions\"\n\t\t\t\t\t\t\t\tv-bind:key=\"index\"\n\t\t\t\t\t\t\t\t:label=\"item\"\n\t\t\t\t\t\t\t\t:value=\"item\">\n\t\t\t\t\t\t\t</el-option>\n\t\t\t\t\t\t</el-select>\n\t\t\t\t\t</el-form-item>\n\t\t\t\t\t<el-form-item class=\"list-item\" v-if=\"tableName=='renshizhuguan'\">\r\n\t\t\t\t\t\t<div class=\"lable\" :class=\"changeRules('touxiang')?'required':''\">头像：</div>\r\n\t\t\t\t\t\t<file-upload\n\t\t\t\t\t\t\ttip=\"点击上传头像\"\n\t\t\t\t\t\t\taction=\"file/upload\"\n\t\t\t\t\t\t\t:limit=\"3\"\n\t\t\t\t\t\t\t:multiple=\"true\"\n\t\t\t\t\t\t\t:fileUrls=\"ruleForm.touxiang?ruleForm.touxiang:''\"\n\t\t\t\t\t\t\t@change=\"renshizhuguantouxiangUploadChange\"\n\t\t\t\t\t\t></file-upload>\n\t\t\t\t\t</el-form-item>\n\t\t\t\t\t<el-form-item class=\"list-item\" v-if=\"tableName=='renshizhuguan'\">\r\n\t\t\t\t\t\t<div class=\"lable\" :class=\"changeRules('lianxidianhua')?'required':''\">联系电话：</div>\r\n\t\t\t\t\t\t<el-input  v-model=\"ruleForm.lianxidianhua\"  autocomplete=\"off\" placeholder=\"联系电话\"  type=\"text\"  />\r\n\t\t\t\t\t</el-form-item>\n\t\t\t\t\t<div class=\"register-btn\">\r\n\t\t\t\t\t\t<div class=\"register-btn1\">\n\t\t\t\t\t\t\t<button type=\"button\" class=\"r-btn\" @click=\"login()\">注册</button>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t<div class=\"register-btn2\">\r\n\t\t\t\t\t\t\t<div class=\"r-login\" @click=\"close()\">已有账号，点击登录</div>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</el-form>\n\t\t</div>\n\t</div>\n</template>\r\n\n<script>\r\n\timport 'animate.css'\r\nexport default {\n\tdata() {\n\t\treturn {\n\t\t\truleForm: {\n\t\t\t},\n\t\t\tforgetForm: {},\n            pageFlag : '',\n\t\t\ttableName:\"\",\n\t\t\trules: {},\n            yuangongxingbieOptions: [],\n            yuangongbumenOptions: [],\n            renshizhuguanxingbieOptions: [],\n\t\t};\n\t},\n\tmounted(){\n\t\tthis.pageFlag = this.$route.query.pageFlag\r\n\t\tif(this.$route.query.pageFlag=='register'){\n\t\t\t\n\t\t\tlet table = this.$storage.get(\"loginTable\");\n\t\t\tthis.tableName = table;\r\n\t\t\tif(this.tableName=='yuangong'){\r\n\t\t\t\tthis.ruleForm = {\r\n\t\t\t\t\tyuangongzhanghao: '',\r\n\t\t\t\t\tmima: '',\r\n\t\t\t\t\tyuangongxingming: '',\r\n\t\t\t\t\txingbie: '',\r\n\t\t\t\t\tlianxidianhua: '',\r\n\t\t\t\t\ttouxiang: '',\r\n\t\t\t\t\tbumen: '',\r\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t\tif(this.tableName=='renshizhuguan'){\r\n\t