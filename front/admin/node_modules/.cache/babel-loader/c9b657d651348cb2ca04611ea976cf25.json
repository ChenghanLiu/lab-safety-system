n\t\t\t\t\t<el-input v-model=\"ruleForm.yuangongxingming\" placeholder=\"员工姓名\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"性别\" prop=\"xingbie\" >\r\n\t\t\t\t\t<el-select :disabled=\"ro.xingbie\" v-model=\"ruleForm.xingbie\" placeholder=\"请选择性别\" >\r\n\t\t\t\t\t\t<el-option\r\n\t\t\t\t\t\t\tv-for=\"(item,index) in xingbieOptions\"\r\n\t\t\t\t\t\t\tv-bind:key=\"index\"\r\n\t\t\t\t\t\t\t:label=\"item\"\r\n\t\t\t\t\t\t\t:value=\"item\">\r\n\t\t\t\t\t\t</el-option>\r\n\t\t\t\t\t</el-select>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"性别\" prop=\"xingbie\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.xingbie\"\r\n\t\t\t\t\t\tplaceholder=\"性别\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"联系电话\" prop=\"lianxidianhua\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.lianxidianhua\" placeholder=\"联系电话\" clearable  :readonly=\"ro.lianxidianhua\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"联系电话\" prop=\"lianxidianhua\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.lianxidianhua\" placeholder=\"联系电话\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"upload\" v-if=\"type!='info' && !ro.touxiang\" label=\"头像\" prop=\"touxiang\" >\r\n\t\t\t\t\t<file-upload\r\n\t\t\t\t\t\ttip=\"点击上传头像\"\r\n\t\t\t\t\t\taction=\"file/upload\"\r\n\t\t\t\t\t\t:limit=\"3\"\r\n\t\t\t\t\t\t:multiple=\"true\"\r\n\t\t\t\t\t\t:fileUrls=\"ruleForm.touxiang?ruleForm.touxiang:''\"\r\n\t\t\t\t\t\t@change=\"touxiangUploadChange\"\r\n\t\t\t\t\t></file-upload>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"upload\" v-else-if=\"ruleForm.touxiang\" label=\"头像\" prop=\"touxiang\" >\r\n\t\t\t\t\t<img v-if=\"ruleForm.touxiang.substring(0,4)=='http'&&ruleForm.touxiang.split(',w').length>1\" class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" :src=\"ruleForm.touxiang\" width=\"100\" height=\"100\">\r\n\t\t\t\t\t<img v-else-if=\"ruleForm.touxiang.substring(0,4)=='http'\" class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" :src=\"ruleForm.touxiang.split(',')[0]\" width=\"100\" height=\"100\">\r\n\t\t\t\t\t<img v-else class=\"upload-img\" style=\"margin-right:20px;\" v-bind:key=\"index\" v-for=\"(item,index) in ruleForm.touxiang.split(',')\" :src=\"$base.url+item\" width=\"100\" height=\"100\">\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"select\" v-if=\"type!='info'\"  label=\"部门\" prop=\"bumen\" >\r\n\t\t\t\t\t<el-select :disabled=\"ro.bumen\" v-model=\"ruleForm.bumen\" placeholder=\"请选择部门\" >\r\n\t\t\t\t\t\t<el-option\r\n\t\t\t\t\t\t\tv-for=\"(item,index) in bumenOptions\"\r\n\t\t\t\t\t\t\tv-bind:key=\"index\"\r\n\t\t\t\t\t\t\t:label=\"item\"\r\n\t\t\t\t\t\t\t:value=\"item\">\r\n\t\t\t\t\t\t</el-option>\r\n\t\t\t\t\t</el-select>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"部门\" prop=\"bumen\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.bumen\"\r\n\t\t\t\t\t\tplaceholder=\"部门\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</template>\r\n\t\t\t<el-form-item class=\"btn\">\r\n\t\t\t\t<el-button class=\"btn3\"  v-if=\"type!='info'\" type=\"success\" @click=\"onSubmit\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t提交\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"btn4\" v-if=\"type!='info'\" type=\"success\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t取消\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"btn5\" v-if=\"type=='info'\" type=\"success\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t返回\r\n\t\t\t\t</el-button>\r\n\t\t\t</el-form-item>\r\n\t\t</el-form>\r\n    \r\n\r\n\t</div>\r\n</template>\r\n<script>\r\n\timport { \r\n\t\tisMobile,\r\n\t} from \"@/utils/validate\";\r\n\texport default {\r\n\t\tdata() {\r\n\t\t\tvar validateMobile = (rule, value, callback) => {\r\n\t\t\t\tif(!value){\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t} else if (!isMobile(value)) {\r\n\t\t\t\t\tcallback(new Error(\"请输入正确的手机号码\"));\r\n\t\t\t\t} else {\r\n\t\t\t\t\tcallback();\r\n\t\t\t\t}\r\n\t\t\t};\r\n\t\t\treturn {\r\n\t\t\t\tid: '',\r\n\t\t\t\ttype: '',\r\n\t\t\t\r\n\t\t\t\r\n\t\t\t\tro:{\r\n\t\t\t\t\tyuangongzhanghao : false,\r\n\t\t\t\t\tmima : false,\r\n\t\t\t\t\tyuangongxingming : false,\r\n\t\t\t\t\txingbie : false,\r\n\t\t\t\t\tlianxidianhua : false,\r\n\t\t\t\t\ttouxiang : false,\r\n\t\t\t\t\tbumen : false,\r\n\t\t\t\t},\r\n\t\t\t\r\n\t\t\t\truleForm: {\r\n\t\t\t\t\tyuangongzhanghao: '',\r\n\t\t\t\t\tmima: '',\r\n\t\t\t\t\tyuangongxingming: '',\r\n\t\t\t\t\txingbie: '',\r\n\t\t\t\t\tlianxidianhua: '',\r\n\t\t\t\t\ttouxiang: '',\r\n\t\t\t\t\tbumen: '',\r\n\t\t\t\t},\r\n\t\t\t\txingbieOptions: [],\r\n\t\t\t\tbumenOptions: [],\r\n\r\n\t\t\t\trules: {\r\n\t\t\t\t\tyuangongzhanghao: [\r\n\t\t\t\t\t\t{ required: true, message: '员工账号不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tmima: [\r\n\t\t\t\t\t\t{ required: true, message: '密码不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tyuangongxingming: [\r\n\t\t\t\t\t\t{ required: true, message: '员工姓名不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\txingbie: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tlianxidianhua: [\r\n\t\t\t\t\t\t{ validator: validateMobile, trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\ttouxiang: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tbumen: [\r\n\t\t\t\t\t],\r\n\t\t\t\t},\r\n\t\t\t};\r\n\t\t},\r\n\t\tprops: [\"parent\"],\r\n\t\tcomputed: {\r\n\r\n\r\n\r\n\t\t},\r\n\t\tcomponents: {\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\t// 下载\r\n\t\t\tdownload(file){\r\n\t\t\t\twindow.open(`${file}`)\r\n\t\t\t},\r\n\t\t\t// 初始化\r\n\t\t\tinit(id,type) {\r\n\t\t\t\tif (id) {\r\n\t\t\t\t\tthis.id = id;\r\n\t\t\t\t\tthis.type = type;\r\n\t\t\t\t}\r\n\t\t\t\tif(this.type=='info'|