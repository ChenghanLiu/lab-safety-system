aceholder=\"账号\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"姓名\" prop=\"xingming\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.xingming\" placeholder=\"姓名\" clearable  :readonly=\"ro.xingming\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"姓名\" prop=\"xingming\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.xingming\" placeholder=\"姓名\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"员工账号\" prop=\"yuangongzhanghao\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.yuangongzhanghao\" placeholder=\"员工账号\" clearable  :readonly=\"ro.yuangongzhanghao\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"员工账号\" prop=\"yuangongzhanghao\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.yuangongzhanghao\" placeholder=\"员工账号\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"员工姓名\" prop=\"yuangongxingming\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.yuangongxingming\" placeholder=\"员工姓名\" clearable  :readonly=\"ro.yuangongxingming\"></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t\t<el-form-item v-else class=\"input\" label=\"员工姓名\" prop=\"yuangongxingming\" >\r\n\t\t\t\t\t<el-input v-model=\"ruleForm.yuangongxingming\" placeholder=\"员工姓名\" readonly></el-input>\r\n\t\t\t\t</el-form-item>\r\n\t\t\t</template>\r\n\t\t\t<el-form-item v-if=\"type!='info'\"  label=\"参加详情\" prop=\"canjiaxiangqing\" >\r\n\t\t\t\t<editor \r\n\t\t\t\t\tstyle=\"min-width: 200px; max-width: 600px;\"\r\n\t\t\t\t\tv-model=\"ruleForm.canjiaxiangqing\" \r\n\t\t\t\t\tclass=\"editor\" \r\n\t\t\t\t\taction=\"file/upload\">\r\n\t\t\t\t</editor>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item v-else-if=\"ruleForm.canjiaxiangqing\" label=\"参加详情\" prop=\"canjiaxiangqing\" >\r\n\t\t\t\t<span class=\"text ql-snow ql-editor\" v-html=\"ruleForm.canjiaxiangqing\"></span>\r\n\t\t\t</el-form-item>\r\n\t\t\t<el-form-item class=\"btn\">\r\n\t\t\t\t<el-button class=\"btn3\"  v-if=\"type!='info'\" type=\"success\" @click=\"onSubmit\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t提交\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"btn4\" v-if=\"type!='info'\" type=\"success\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t取消\r\n\t\t\t\t</el-button>\r\n\t\t\t\t<el-button class=\"btn5\" v-if=\"type=='info'\" type=\"success\" @click=\"back()\">\r\n\t\t\t\t\t<span class=\"icon iconfont icon-xihuan\"></span>\r\n\t\t\t\t\t返回\r\n\t\t\t\t</el-button>\r\n\t\t\t</el-form-item>\r\n\t\t</el-form>\r\n    \r\n\r\n\t</div>\r\n</template>\r\n<script>\r\n\texport default {\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tid: '',\r\n\t\t\t\ttype: '',\r\n\t\t\t\r\n\t\t\t\r\n\t\t\t\tro:{\r\n\t\t\t\t\tpeixunmingcheng : false,\r\n\t\t\t\t\tpeixunleixing : false,\r\n\t\t\t\t\tpeixuntupian : false,\r\n\t\t\t\t\tcanjiashijian : false,\r\n\t\t\t\t\tcanjiaxiangqing : false,\r\n\t\t\t\t\tzhanghao : false,\r\n\t\t\t\t\txingming : false,\r\n\t\t\t\t\tyuangongzhanghao : false,\r\n\t\t\t\t\tyuangongxingming : false,\r\n\t\t\t\t},\r\n\t\t\t\r\n\t\t\t\truleForm: {\r\n\t\t\t\t\tpeixunmingcheng: '',\r\n\t\t\t\t\tpeixunleixing: '',\r\n\t\t\t\t\tpeixuntupian: '',\r\n\t\t\t\t\tcanjiashijian: '',\r\n\t\t\t\t\tcanjiaxiangqing: '',\r\n\t\t\t\t\tzhanghao: '',\r\n\t\t\t\t\txingming: '',\r\n\t\t\t\t\tyuangongzhanghao: '',\r\n\t\t\t\t\tyuangongxingming: '',\r\n\t\t\t\t},\r\n\r\n\t\t\t\trules: {\r\n\t\t\t\t\tpeixunmingcheng: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tpeixunleixing: [\r\n\t\t\t\t\t\t{ required: true, message: '培训类型不能为空', trigger: 'blur' },\r\n\t\t\t\t\t],\r\n\t\t\t\t\tpeixuntupian: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tcanjiashijian: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tcanjiaxiangqing: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\tzhanghao: [\r\n\t\t\t\t\t],\r\n\t\t\t\t\txingming: